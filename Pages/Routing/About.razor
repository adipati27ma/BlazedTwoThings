@page "/about"
@page "/about-me"

@* kalo int, bisa pake "int?", kalo string ga bisa pake constraint *@
@page "/about/{parameter1}/{parameter2}/{parameter3:int?}"

<h2>About Page</h2>

<a href="/" class="btn btn-primary">Back To Index</a>
<br />
<br />
<p>param 1 = @Parameter1</p>
<p>param 2 = @Parameter2</p>
<p>param 3 = @Parameter3.ToString("n0") (integer)</p>

@code {
  [Parameter]
  public string Parameter1 { get; set; }
  [Parameter]
  public string Parameter2 { get; set; }
  [Parameter]
  public int Parameter3 { get; set; }
}